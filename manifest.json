{
  "manifest_version": 2,

  "name": "Material Color Chrome",
  "description": "Color Selector in Chrome",
  "options_page": "options.html",
  "version": "1.0.0",
  "icons": { "512": "icon.png" },

  "browser_action": {"default_title": "Material Color Picker",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}